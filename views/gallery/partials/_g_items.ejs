
<% categories.forEach(function(category, category_val) { %>

	<div class="container_960">

	<div class="col-xs-12 col-sm-12 col-md-12">
		<div class="b-gallery-tytle">
			<%= category.name %>
		</div>
	</div>	

	<div class="row">
		
		<% category.items.forEach(function(item, val) { %>

			 <div class="col-xs-12 col-sm-3 col-md-3">
			 	<a data-lightbox="image-<%= category_val %>" href="<%= item.big_url %>" >
			 		<img src="<%= item.big_url %>" class="staff_img" />
			 	</a>	
			 </div>

		<% }) %>

	</div>
</div>

<% }) %>

