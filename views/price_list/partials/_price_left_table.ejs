<div class="container_960">
	<div class="row">

		<div class="col-xs-12 col-sm-12 col-md-12">
			<div class="b-gallery-tytle">
				EPILACJA IPL
			</div>
		</div>	

		<div class="col-xs-12 col-sm-4 col-md-4">
			<img class="staff_img" src="/img/popular/3.jpg" />
		</div>
		
		<div class="col-xs-12 col-sm-8 col-md-8">
			<div>
				<table class="price_table">
					<tr class="first_tr">
						<td></td>
						<td>MAŁE PARTIE</td>
						<td>ŚREDNIE PARTIE</td>
						<td>DUŻE PARTIE</td>
					</tr>

					<% epil_items.forEach( function(epil) {%>

						<tr>
							<td><%= epil.service_type_name %></td>
							<td class="price_big_td"><%= epil.value_1 %> zł</td>
							<td class="price_big_td"><%= epil.value_2 %> zł</td>
							<td class="price_big_td"><%= epil.value_3 %> zł</td>
						</tr>

					<%})%>
				</table>
			</div>	

		</div>

	</div>	
</div>	